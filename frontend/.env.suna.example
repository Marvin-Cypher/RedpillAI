# Suna AI Integration Configuration
# Copy this to .env.local and fill in your values

# Existing RedpillAI Config (keep these)
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXTAUTH_SECRET=your-secret-here
NEXTAUTH_URL=http://localhost:3000

# Suna AI Backend (new)
# For local Suna deployment
SUNA_API_URL=http://localhost:8001
SUNA_API_KEY=your-suna-api-key

# For Suna Cloud (when available)
# SUNA_API_URL=https://api.suna.ai
# SUNA_API_KEY=your-cloud-api-key

# Optional: If you want to expose Suna config to client
# NEXT_PUBLIC_SUNA_API_URL=http://localhost:8001
# NEXT_PUBLIC_SUNA_API_KEY=your-suna-api-key

# Keep existing AI provider (as fallback)
NEXT_PUBLIC_REDPILL_AI_API_KEY=your-deepseek-key

# Market data (keep existing)
NEXT_PUBLIC_COINGECKO_API_KEY=your-coingecko-key

# Migration Flag - set to true to use Suna
USE_SUNA_BACKEND=false

# For testing - use mock Suna client
NEXT_PUBLIC_USE_SUNA_MOCK=true